{
  "name": "@elghandour/fetchify",
  "version": "1.1.2-beta.7",
  "private": false,
  "description": "Universal wrapper for Fetch API",
  "main": "./dist/browser/build.esm.js",
  "types": "./dist/types/main.d.ts",
  "typings": "./dist/types/main.d.ts",
  "jsdelivr": "dist/build.umd.js",
  "unpkg": "dist/build.umd.js",
  "engines": {
    "node": ">=14.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedElghandour1/fetchify.git"
  },
  "bugs": {
    "url": "https://github.com/ahmedElghandour1/fetchify/issues",
    "email": "elghandour.12@gmail.com"
  },
  "homepage": "https://github.com/ahmedElghandour1/fetchify#readme",
  "author": "Ahmed Elghandour",
  "license": "MIT",
  "keywords": [
    "fetch",
    "fetchify",
    "browser",
    "node",
    "nodejs",
    "fetch wrapper",
    "universal fetch",
    "http",
    "promise"
  ],
  "scripts": {
    "server": "nodemon ./server/index.js",
    "build": "cross-env NODE_ENV=production node ./scripts/esbuild.config.mjs && tsc --emitDeclarationOnly",
    "watch:types": "tsc --emitDeclarationOnly --watch",
    "watch:ts": "cross-env WATCH=1 NODE_ENV=development  node ./scripts/esbuild.config.mjs",
    "watch": "concurrently  \"npm:watch:types\" \"npm:watch:ts\"",
    "preinstall": "npx only-allow pnpm",
    "prepublish": "npm run build",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.mjs",
    "dist/types/main.d.ts",
    "*"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/browser/build.esm.js"
      },
      "require": {
        "default": "./dist/browser/build.common.js"
      },
      "types": "./dist/types/main.d.ts"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./nodejs-polyfills.mjs": "./polyfills/nodejs-polyfills.mjs"
  },
  "dependencies": {
    "@types/qs": "^6.9.7",
    "express": "^4.17.3",
    "form-data": "^4.0.0",
    "install": "^0.13.0",
    "node-fetch": "^3.2.10",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.32",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.50",
    "lerna": "^5.5.4",
    "nodemon": "^2.0.15",
    "nx": "14.8.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  }
}
